{
  "routes": [
    { 
      "src": "/api/(.*)", 
      "dest": "/api/index.ts" 
    },
    { 
      "src": "/(.*)", 
      "dest": "/api/index.ts" 
    }
  ],
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "outputDirectory": "client/dist"
}